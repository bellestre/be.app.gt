@model List<Wapp.gt.Models.MisEventos>
<section class="content-header">
    <h1>
        Home
        <small>Pagina de inicio</small>
    </h1>
</section>
<!-- Main content -->
<section class="content container-fluid">
    <div class="alert alert-info alert-dismissible">
        <button type="button" class="close" data-dismiss="alert" aria-hidden="true">x</button>
        <h4><i class="icon fa fa-check"></i> Bienvenido!</h4>
        Este es el portal de grados y titulos de la FIIS - UNAS
    </div>
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">Fechas de ejecucion de las tesis</h3>
        </div>
        <!-- /.box-header -->
        <div class="box-body table-responsive no-padding">
            <table class="table table-hover">
                <tr>
                    <th>Tema</th>
                    <th>Fecha de inicio</th>
                    <th>Fecha de fin</th>
                </tr>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@Html.DisplayFor(modelItem => item.Nombre)</td>
                        <td>@Html.DisplayFor(modelItem => item.F_inicio)</td>
                        <td>@Html.DisplayFor(modelItem => item.F_fin)</td>
                    </tr>
                 }
            </table>
        </div>
        <div class="box-footer clearfix">
            <ul class="pagination pagination-sm no-margin pull-right">
                <li><a href="#">&laquo;</a></li>
                <li><a href="#">1</a></li>
                <li><a href="#">2</a></li>
                <li><a href="#">3</a></li>
                <li><a href="#">&raquo;</a></li>
            </ul>
        </div>
        <!-- /.box-body -->
    </div>
</section>