<script>
    function MyMapita() {
        var mapProp = {
            center: new google.maps.LatLng(-9.313168, -75.996661),
            zoom: 15,

        };
        var map = new google.maps.Map(document.getElementById("googleMap"), mapProp);

        var marker = new google.maps.Marker({
            position: new google.maps.position(-9.313168, -75.996661),
            icon: 'pinkball.png'
        });
        marker.setMap(map);


    }
</script>
<script type="text/javascript" src="https://maps.google.com/maps/api/js?key=AIzaSyASq8YmttACgZGX3HVwu-8eB37gtmhYqmY&callback=MyMapita"></script>
